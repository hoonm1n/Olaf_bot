<?xml version="1.0"?>

<launch>
  <!-- move_base -->
    <node pkg="move_base" type="move_base" respawn="false" name="move_base" output="screen">
        <param name="base_local_planner" value="dwa_local_planner/DWAPlannerROS" />
        <rosparam file="$(find hospital_bot)/param/costmap_common_params.yaml" command="load" ns="global_costmap" />
        <rosparam file="$(find hospital_bot)/param/costmap_common_params.yaml" command="load" ns="local_costmap" />
        <rosparam file="$(find hospital_bot)/param/local_costmap_params.yaml" command="load" />
        <rosparam file="$(find hospital_bot)/param/global_costmap_params.yaml" command="load" />
        <rosparam file="$(find hospital_bot)/param/move_base_params.yaml" command="load" />
        <rosparam file="$(find hospital_bot)/param/dwa_local_planner_params.yaml" command="load" />
    </node>
    
</launch>